msgstr "Archivo"

#: temp.glade:18282
msgid "Frame"
msgstr "Marco"

#: temp.glade:18296
msgid "Scale"
msgstr "Escala"

#: temp.glade:18310
msgid "Posn"
msgstr "Posada"

#: temp.glade:18325

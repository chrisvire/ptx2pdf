msgstr "125mm, 176mm (B6)"

#: temp.glade:4365
msgid "210mm, 280mm (PA4)"
msgstr "210mm, 280mm (PA4)"

#: temp.glade:4366
msgid "140mm, 210mm (PA5)"
msgstr "140mm, 210mm (PA5)"

